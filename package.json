  "license": "MIT",
  "private": false,
  "dependencies": {
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "node-serialize": "^0.0.4"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
